							<li>
								<a href="{{path('app_home')}}">HOME</a>
							</li>
							<li>
								<a href="">Management</a>
								<ul class="menu_child">
									<li>
										<a href="{{path('app_category')}}">Category</a>
									</li>
									<li>
										<a href="{{path('app_product')}}">Product</a>
									</li>

								</ul>
							</li>


							<li>
								<a href="{{path('app_login')}}">Sign in</a>
							</li>
							<li>
								<a href="{{path('app_registration')}}">Register</a>
							</li>

						</ul>
                                   	</div>
                           	</div>
                    	</div>
          </header>
          <footer>
                  <div class="container">
